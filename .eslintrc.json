{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "max-len": [
      "error",
      {
        "code": 128
      }
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ]
  }
}